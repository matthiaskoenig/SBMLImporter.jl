            </apply>
          </apply>
        </math>
            </assignmentRule>
      <assignmentRule variable="G_gut">
        <math xmlns="http://www.w3.org/1998/Math/MathML">        
          <apply>
            <times/>
            <ci> k02 </ci>
            <apply>
              <divide/>
              <ci> f_G </ci>
              <apply>
                <times/>
                <ci> V_G </ci>
                <ci> BW </ci>
              </apply>
            </apply>
            <ci> M_G_gut </ci>
          </apply>
        </math>
            </assignmentRule>
      <assignmentRule variable="U_ii">
        <math xmlns="http://www.w3.org/1998/Math/MathML">        
          <apply>
            <times/>
            <ci> G_liv_b </ci>
            <apply>
              <divide/>
              <apply>
                <plus/>
                <ci> KM </ci>
                <ci> G_b </ci>
              </apply>
              <ci> G_b </ci>
            </apply>
            <apply>
              <divide/>
              <ci> G_PL </ci>
              <apply>
                <plus/>
                <ci> KM </ci>
                <ci> G_PL </ci>
              </apply>
            </apply>
          </apply>
        </math>
            </assignmentRule>
      <assignmentRule variable="U_ren">
        <math xmlns="http://www.w3.org/1998/Math/MathML">        
          <piecewise>
            <piece>
              <cn> 0 </cn>
              <apply>
                <lt/>
                <ci> G_PL </ci>
                <ci> G_th_PL </ci>
              </apply>
            </piece>
            <otherwise>
              <apply>
                <times/>
                <apply>
                  <divide/>
                  <ci> c1 </ci>
                  <apply>
                    <times/>
                    <ci> V_G </ci>
                    <ci> BW </ci>
                  </apply>
                </apply>
                <apply>
                  <minus/>
                  <ci> G_PL </ci>
                  <ci> G_th_PL </ci>
                </apply>
              </apply>
            </otherwise>
          </piecewise>
        </math>
            </assignmentRule>
      <assignmentRule variable="I_liv">
        <math xmlns="http://www.w3.org/1998/Math/MathML">        
          <apply>
            <times/>
            <ci> k07 </ci>
            <apply>
              <divide/>
              <ci> G_b </ci>
